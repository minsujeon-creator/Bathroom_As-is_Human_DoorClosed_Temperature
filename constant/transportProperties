FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      transportProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

transportModel  Newtonian;

nu              [0 2 -1 0 0 0 0]    1.5e-05;
DfreshAir       [0 2 -1 0 0 0 0]    1e-05;          // laminar diffusivity [m²/s]
DH2S            [0 2 -1 0 0 0 0]    1.414e-5;   // m2/s
DNH3            [0 2 -1 0 0 0 0]    1.98e-5;    // m2/s

Pr              0.71;                       // laminar Pr
Prt             0.85;                       // turbulent Prandtl (alphat = nu_t/Prt)

rhoRef  [1 -3 0 0 0 0 0] 1.225;
beta    [0 0 0 -1 0 0 0] 3.3e-3;   // 1/K  (≈ 1/T for air around room temp)
TRef    [0 0 0 1 0 0 0]  300;      // K

species
(
    freshAir
    H2S
    NH3
);


// ************************************************************************* //
