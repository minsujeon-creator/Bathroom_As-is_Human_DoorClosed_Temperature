FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    walls   { type calculated; value $internalField; }
    room    { type calculated; value $internalField; }
    human   { type calculated; value $internalField; }
    vent  { type calculated; value $internalField; }
    door   { type calculated; value $internalField; }
    poo    { type calculated; value $internalField; }
}


