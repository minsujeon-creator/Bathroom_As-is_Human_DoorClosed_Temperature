FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      alphat;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];   // m2/s
internalField   uniform 0;

boundaryField
{
    walls   { type calculated; value uniform 0; }
    room    { type calculated; value uniform 0; }
    human   { type calculated; value uniform 0; }
    vent  { type calculated; value uniform 0; }
    door   { type calculated; value uniform 0; }
    poo    { type calculated; value uniform 0; }
}

